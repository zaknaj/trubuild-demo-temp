{
  "uid": "trubuild-infra-logs",
  "title": "TruBuild - Infrastructure Logs",
  "description": "Logs from Nginx, PostgreSQL, MinIO, Loki, Promtail, Grafana, and other infrastructure services",
  "tags": ["trubuild", "infrastructure", "nginx", "postgres", "minio", "loki", "promtail", "grafana"],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "id": 1,
      "title": "Nginx Log Volume",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"nginx\"} [1m]))",
          "legendFormat": "Nginx logs / min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 50 },
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "title": "PostgreSQL Log Volume",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"postgres\"} [1m]))",
          "legendFormat": "Postgres logs / min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 50 },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "MinIO Log Volume",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"minio\"} [1m]))",
          "legendFormat": "MinIO logs / min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 50 },
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Nginx Error Logs",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"nginx\"} |~ \"(?i)(error|warn|crit|emerg)\" [5m]))",
          "legendFormat": "Nginx Errors / 5min",
          "refId": "A"
        },
        {
          "expr": "sum(count_over_time({service=\"nginx\"} |~ \"\\\" 5[0-9]{2} \" [5m]))",
          "legendFormat": "5xx Responses / 5min",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80 },
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Database Errors & Slow Queries",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 6 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({service=\"postgres\"} |~ \"(?i)(error|fatal|panic)\" [5m]))",
          "legendFormat": "DB Errors / 5min",
          "refId": "A"
        },
        {
          "expr": "sum(count_over_time({service=\"postgres\"} |~ \"(?i)duration:\" [5m]))",
          "legendFormat": "Slow Queries / 5min",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 80 },
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Nginx Logs",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{service=\"nginx\"} |~ \"$search\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
